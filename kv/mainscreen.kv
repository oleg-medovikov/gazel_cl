<MainScreen>:
    BoxLayout:
        orientation: 'vertical'
		spacing: cm(1.5)
		padding_horizontal: cm(1)
		BoxLayout:
			orientation: 'horizontal'
			size_hint_y: None
			height: '2cm'
			Label:
				id: hello
				text: "Hello"
				font_size: '0.5cm'

		BoxLayout:
			orientation: 'horizontal'

		BoxLayout:
			orientation: 'horizontal'
			size_hint_y: None
			height: cm(3.5)
			padding: cm(1)
			spacing: cm(1)
			Button:
				id: logout
				text: 'Выйти'
				on_press: root.logout()
			Button:
				id: users
				text: 'Пользователи'
				on_press: root.see_users()
